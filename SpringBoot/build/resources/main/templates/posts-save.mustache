{{>layout/header}}
<h1> </h1>
<div class="input_page">
    <form>
        <div class="form_group">
            <input type="text" class="input" id="title" placeholder="제목을 입력하세요">
        </div>
        <div class="form_group">
            <input type="text" class="input" id="author" placeholder="작성자를 입력하세요">
        </div>
        <div class="form_group">
            <textarea class="text" id="content" placeholder="내용을 입력하세요"></textarea>
        </div>
    </form>
    <div class="input_below">
        <a href="/" role="button" id="a_cancel_button">취소</a>
        <button type="button" class="button" id="btn-save">등록</button>
    </div>
</div>
{{>layout/footer}}