{{>layout/header}}
<h1>관리자 댓글보기</h1>
<div class="col-md-12">
    <div class="row">
        <div class="col-md-6">
            <a href = "/admin/comments" role="button" class="btn btn-primary">돌아가기</a>
        </div>
    </div>
    <br>
    <table class="table table-horizontal table-bordered" id="commentTable">
        <thead class="thrad-strong">
        <tr>
            <th>댓글</th>
            <th>신고 내용</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#report}}
            <tr>
                <td>{{content}}</td>
                <td>{{words}}</td>
            </tr>
        {{/report}}
        </tbody>
    </table>
</div>
{{>layout/footer}}