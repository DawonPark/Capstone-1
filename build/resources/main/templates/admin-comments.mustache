{{>layout/header}}
<h1>관리자 댓글보기</h1>
<div class="col-md-12">
    <table class="comment_table" id="commentTable">
        <thead class="comment_table_head">
        <tr>
            <th>게시글 번호</th>
            <th>댓글 번호</th>
            <th>작성자</th>
            <th>내용</th>
            <th>날짜</th>
            <th>신고</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#comment}}
            <tr>
                <td>{{pid}}</td>
                <td>{{id}}</td>
                <td>{{author}}</td>
                <td>{{content}}</td>
                <td>{{modifiedDate}}</td>
                <td>{{report}}</td>
            </tr>
        {{/comment}}
        </tbody>
    </table>
    <div class="below_table">
            <a href = "/admin" role="button" class="a_button">돌아가기</a>
    </div>

</div>
{{>layout/footer}}